{
  "initHook" : "org.chenile.jgen.blueprint.minimonolith.InitMinimonolithBlueprint",
  "name" : "minimonolith",
  "description" : "Generates a New Mini Monolith",
  "templateFolder": "monolith-template",
  "inputFields" :[
    {
      "name" : "monolith",
      "type" : "STRING",
      "description": "Name of mini monolith"
    },
    {
    "name" : "monolithVersion",
    "type" : "STRING",
    "description": "Version of mini monolith",
    "defaultValue" : "${defaultVersion}"
    },
    {
      "name" : "destFolder",
      "type" : "STRING",
      "description": "Destination Folder",
      "defaultValue" : "${defaultDestFolder}"
    }
    ,{
        "name" : "jpa",
        "type" : "BOOLEAN",
        "description": "Enable JPA",
        "defaultValue" : "y"
    }
  ,
    {
      "name" : "cloudSwitchEnabled",
      "type" : "BOOLEAN",
      "description": "Enable Cloud Switch",
      "defaultValue" : "n"
    },
    {
      "name" : "security",
      "type" : "BOOLEAN",
      "description": "Enable Security",
      "defaultValue" : "n"
    }
  ]
}