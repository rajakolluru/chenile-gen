
chenile:
  security:
    client:
      id: "authz-servlet"
      secret: "secret"
    keycloak:
      host: "http://localhost:8180"
      port: 8180
      base:
        realm: quickstart

spring:
  application:
    name: {{monolith}}
  security:
    oauth2:
      resourceserver:
        jwt:
          jwk-set-uri: http://localhost:8180/realms/#tenantId#/protocol/openid-connect/certs

server:
  banner:
    location: classpath:banner.txt
logging:
  level:
    org:
      chenile:
        security: DEBUG
